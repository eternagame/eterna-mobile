{
  "name": "org.eternagame.mobile",
  "displayName": "EternaMobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild:dev": "cd eternajs && npm run build:dev",
    "build:dev": "webpack",
    "prebuild:prod": "cd eternajs && npm run build:prod",
    "build:prod": "webpack --config webpack.config.prod.js",
    "postbuild:prod": "shx cp -r eternajs/website/frontend www/eternajs/frontend && coffee -o www/eternajs/frontend/jscripts -c www/eternajs/frontend/coffee",
    "watch": "webpack --watch"
  },
  "author": "Eat Create Sleep AB",
  "dependencies": {
    "axios": "^0.19.2",
    "boostrap": "^2.0.0",
    "bootstrap-vue": "^2.14.0",
