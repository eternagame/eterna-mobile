{
  "name": "org.eternagame.mobile",
  "displayName": "EternaMobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "shx mkdir -p www && (is-ci && cd eternajs && npm ci) || (is-ci || cd eternajs && npm install)",
    "prebuild:dev": "npm run clean && cd eternajs && npm run build:dev",
    "build:dev": "webpack --config webpack.config.dev.js",
    "postbuild:dev": "npm run postbuild",
    "prebuild:prod": "npm run clean && cd eternajs && npm run build:prod",
    "build:prod": "webpack --config webpack.config.prod.js",
    "postbuild:prod": "npm run postbuild",
    "postbuild": "shx mkdir -p www/eternajs/frontend && shx cp -r eternajs/website/frontend/* www/eternajs/frontend && coffee -o www/eternajs/frontend/jscripts -c www/eternajs/frontend/coffee",
    "clean": "shx rm -r www/* || true"
  },
  "author": "Eat Create Sleep AB",
  "dependencies": {
    "axios": "^0.21.0",
    "boostrap": "^2.0.0",
    "bootstrap-vue": "^2.21.0",
    "cordova-android": "^9.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-ionic-keyboard": "2.2.0",
    "cordova-plugin-navigationbar": "^1.0.31",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "es6-promise-plugin": "^4.2.2",
    "eterna-chat-wrapper": "github:eternagame/eterna-chat-wrapper#abe53eca223948847b4f899f9de7119672adf3a2",
    "jquery": "^3.5.1",
    "vue": "^2.6.12",
    "vue-axios": "^3.2.0",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@npmcli/run-script": "^1.8.1",
    "@types/node": "^14.14.14",
    "@types/webpack": "^4.41.25",
    "coffeescript": "^1.12.7",
    "copy-webpack-plugin": "^7.0.0",
    "cordova": "^10.0.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-plugin-wkwebview-file-xhr": "^3.0.0",
    "cordova-plugin-wkwebview-inject-cookie": "git+https://github.com/lucky3491/cordova-plugin-wkwebview-inject-cookie.git",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "is-ci": "^2.0.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-navigationbar": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-wkwebview-inject-cookie": {},
      "cordova-plugin-wkwebview-file-xhr": {}
    },
    "platforms": [
      "android",
      "ios",
      "browser"
    ]
  }
}
